  "scripts": {
    "analyze": "cross-env BUNDLE_ANALYZE=both NODE_ENV=production next build src",
    "build": "next build src",
    "build:lang": "next build src && node ./src/lib/defaultLang",
    "build:semantic": "cd src/lib/semantic && gulp build-css build-assets",
    "bundlesize": "bundlesize",
    "compile-queries": "persistgraphql src/graphql/ --add_typename",
    "cypress:open": "cypress open",
    "dev": "cross-env NODE_ENV=development nodemon -w .env -w package.json -w yarn.lock -w server.js -w src/klicker.conf.js server.js",
    "dev:inspect": "cross-env NODE_ENV=development nodemon -w .env -w package.json -w yarn.lock -w server.js -w src/klicker.conf.js --inspect server.js",
    "dev:ndb": "cross-env NODE_ENV=development ndb node server.js",
    "format": "prettier --write \"src/**/*.js\" \"src/**/*.graphql\" \"src/**/*.json\" server.js",
    "format:ci": "prettier --list-different \"src/**/*.js\" \"src/**/*.graphql\" \"src/**/*.json\" server.js",
    "lint": "eslint \"src/**/*.js\" server.js",
    "start": "cross-env NODE_ENV=production node server.js",
    "start:inspect": "cross-env NODE_ENV=production node --inspect server.js",
    "storybook": "cross-env NODE_ENV=development start-storybook -p 6006 -c .storybook",
    "test": "jest",
    "test:ci": "jest --coverage",
    "test:watch": "jest --watch"
  },
  "jest": {
    "testEnvironment": "node",
    "roots": [
      "src/"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "src/.next"
    ]
  },
  "lint-staged": {
    "*.js": [
      "prettier --write",
      "eslint",
      "git add"
    ],
    "*.json": [
      "prettier --write",
      "git add"
    ],
    "*.graphql": [
      "prettier --write",
      "git add"
    ]
  },
  "bundlesize": [
    {
      "path": "./src/.next/static/chunks/commons.*.js"
    },
    {
      "path": "./src/.next/static/css/commons.*.css"
    },
    {
      "path": "./src/.next/static/runtime/main-*.js"
    }
  ],
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "jest --onlyChanged"
    }
  }
}
