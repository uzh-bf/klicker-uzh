    "babel-polyfill": "6.26.0",
    "classnames": "2.2.5",
    "cross-env": "5.0.5",
    "dotenv": "4.0.0",
    "express": "4.16.1",
    "glob": "7.1.2",
    "graphql": "0.10.5",
    "intl": "1.2.5",
    "isomorphic-fetch": "2.2.1",
    "lodash": "4.17.4",
    "logrocket": "0.5.5",
    "logrocket-react": "0.0.4",
    "moment": "2.18.1",
    "next": "3.2.2",
    "prop-types": "15.6.0",
    "prosemirror-schema-basic": "0.24.0",
    "prosemirror-state": "0.24.0",
    "prosemirror-view": "0.24.0",
    "react": "16.0.0",
    "raven-js": "3.18.1",
    "react-addons-css-transition-group": "15.6.2",
    "react-apollo": "1.4.16",
    "react-autosuggest": "9.3.2",
    "react-dnd": "2.5.3",
    "react-dnd-html5-backend": "2.5.3",
    "react-dom": "16.0.0",
    "react-helmet": "5.2.0",
    "react-icons": "2.2.5",
    "react-intl": "2.4.0",
    "react-rangeslider": "2.2.0",
    "react-redux": "5.0.6",
    "react-sortable-hoc": "0.6.8",
    "react-tagsinput": "3.18.0",
    "recharts": "1.0.0-apha.5",
    "redux": "3.7.2",
    "redux-form": "7.0.3",
    "semantic-ui-react": "0.74.2",
    "validator": "9.0.0"
  },
  "devDependencies": {
    "@storybook/addon-actions": "3.2.12",
    "@storybook/addon-info": "3.2.12",
    "@storybook/addon-links": "3.2.12",
    "@storybook/addon-storyshots": "3.2.12",
    "@storybook/react": "3.2.12",
    "babel-eslint": "8.0.1",
    "enzyme": "2.9.1",
    "eslint": "4.8.0",
    "eslint-config-airbnb": "15.1.0",
    "eslint-plugin-import": "2.7.0",
    "eslint-plugin-jsx-a11y": "5.1.1",
    "eslint-plugin-react": "7.4.0",
    "husky": "0.14.3",
    "jest": "21.1.0",
    "lint-staged": "4.2.3",
    "nodemon": "1.12.1",
    "prettier": "1.7.4",
    "prettier-eslint": "8.2.0",
    "prettier-eslint-cli": "4.4.0",
    "react-addons-test-utils": "15.6.0",
    "react-dnd-test-backend": "2.5.3",
    "react-test-renderer": "15.6.1",
    "semantic-ui-css": "2.2.12",
    "storybook-addon-intl": "2.1.1"
  },
  "scripts": {
    "precommit": "lint-staged",
    "prepush": "yarn run test --onlyChanged",
    "build": "next build src && node ./src/lib/defaultLang",
    "dev": "nodemon -w .env -w package.json -w yarn.lock server.js",
    "dev:inspect": "nodemon -w .env -w package.json -w yarn.lock --inspect server.js",
    "format": "prettier-eslint --single-quote --no-semi --write server.js \"src/**/*.js\"",
    "format:ci":
      "prettier-eslint --single-quote --no-semi --list-different server.js \"src/**/*.js\"",
    "lint": "eslint server.js \"src/**/*.js\"",
    "start": "cross-env NODE_ENV=production node server.js",
    "start:inspect": "cross-env NODE_ENV=production node --inspect server.js",
    "storybook": "start-storybook -p 6006",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "jest": {
    "testEnvironment": "node",
    "roots": ["src/"]
  },
  "lint-staged": {
    "*.js": ["prettier-eslint --single-quote --no-semi --write", "eslint", "git add"],
    "*.json": ["prettier-eslint --single-quote --no-semi --write", "git add"]
  }
}
