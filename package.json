  "scripts": {
    "build": "next build src && node ./src/lib/defaultLang",
    "debug": "cross-env NODE_ENV=production node --inspect server.js",
    "dev": "nodemon -w .env -w package.json -w yarn.lock server.js",
    "dev:inspect": "nodemon -w .env -w package.json -w yarn.lock --inspect server.js",
    "flow": "flow check",
    "format": "prettier-eslint --single-quote --no-semi --write server.js \"src/**/*.js\"",
    "format:ci": "prettier-eslint --single-quote --no-semi --list-different server.js \"src/**/*.js\"",
    "lint": "eslint server.js \"src/**/*.js\"",
    "start": "cross-env NODE_ENV=production node server.js",
    "storybook": "start-storybook -p 6006",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "jest": {
    "testEnvironment": "node",
    "roots": [
      "src/"
    ]
  },
  "pre-commit": [
    "lint"
