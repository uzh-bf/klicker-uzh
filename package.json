    "prettier": "1.10.2",
    "prettier-eslint": "8.8.1",
    "prettier-eslint-cli": "4.7.0",
    "react-addons-test-utils": "15.6.2",
    "react-dnd-test-backend": "2.5.4",
    "react-test-renderer": "16.2.0",
    "semantic-ui-css": "2.2.14",
    "storybook-addon-intl": "2.3.0",
    "webpack-bundle-analyzer": "2.10.0"
  },
  "scripts": {
    "precommit": "lint-staged",
    "prepush": "yarn run test --onlyChanged",
    "analyze": "cross-env ANALYZE=1 next build src",
    "build": "next build src && node ./src/lib/defaultLang",
    "compile-queries": "persistgraphql src/graphql/ --add_typename",
    "cypress:open": "cypress open",
    "dev": "nodemon --delay 1000ms -w .env -w package.json -w yarn.lock -w server.js server.js",
    "dev:inspect":
      "nodemon --delay 1000ms -w .env -w package.json -w yarn.lock -w server.js --inspect server.js",
    "format": "prettier-eslint --singleQuote --no-semi --write server.js \"src/**/*.js\"",
    "format:ci":
      "prettier-eslint --singleQuote --no-semi --list-different server.js \"src/**/*.js\"",
    "lint": "eslint server.js \"src/**/*.js\"",
    "start": "cross-env NODE_ENV=production node server.js",
    "start:inspect": "cross-env NODE_ENV=production node --inspect server.js",
    "start:pm": "NODE_ENV=production pm2-docker start --env production server.js",
    "storybook": "start-storybook -p 6006",
    "test": "jest",
    "test:ci": "jest --coverage",
    "test:watch": "jest --watch"
  },
  "jest": {
    "testEnvironment": "node",
    "roots": ["src/"],
    "testPathIgnorePatterns": ["/node_modules/", "src/.next"]
  },
  "lint-staged": {
    "*.js": ["prettier-eslint --single-quote --no-semi --write", "eslint", "git add"],
    "*.json": ["prettier-eslint --single-quote --no-semi --write", "git add"]
  }
}
