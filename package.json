  "scripts": {
    "precommit": "lint-staged",
    "prepush": "yarn run test --onlyChanged",
    "analyze": "cross-env BUNDLE_ANALYZE=both NODE_ENV=development next build src",
    "build": "yarn run build:semantic && next build src",
    "build:lang": "next build src && node ./src/lib/defaultLang",
    "build:semantic": "cd src/lib/semantic && gulp build-css build-assets",
    "bundlesize": "bundlesize",
    "compile-queries": "persistgraphql src/graphql/ --add_typename",
    "cypress:open": "cypress open",
    "dev":
      "cross-env NODE_ENV=development nodemon --delay 1000ms -w .env -w package.json -w yarn.lock -w server.js server.js",
    "dev:inspect":
      "cross-env NODE_ENV=development nodemon --delay 1000ms -w .env -w package.json -w yarn.lock -w server.js --inspect server.js",
    "format": "prettier-eslint --singleQuote --no-semi --write server.js \"src/**/*.js\"",
    "format:ci":
      "prettier-eslint --singleQuote --no-semi --list-different server.js \"src/**/*.js\"",
    "lint": "eslint server.js \"src/**/*.js\"",
    "start": "cross-env NODE_ENV=production node server.js",
    "start:inspect": "cross-env NODE_ENV=production node --inspect server.js",
    "storybook": "cross-env NODE_ENV=development start-storybook -p 6006 -c .storybook",
    "test": "jest",
    "test:ci": "jest --coverage",
    "test:watch": "jest --watch"
  },
  "jest": {
    "testEnvironment": "node",
    "roots": ["src/"],
    "testPathIgnorePatterns": ["/node_modules/", "src/.next"]
  },
  "lint-staged": {
    "*.js": ["prettier-eslint --single-quote --no-semi --write", "eslint", "git add"],
    "*.json": ["prettier-eslint --single-quote --no-semi --write", "git add"]
  },
  "bundlesize": [
    {
      "path": "./src/.next/static/commons/main-*.js"
    }
  ]
}
