{
  "name": "@klicker-uzh/prisma",
  "version": "2.0.0-alpha.22",
  "license": "AGPL-3.0",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "analyze": "size-limit --why",
    "build:all": "npm-run-all generate build",
    "build": "npm-run-all build:ts build:copy",
    "build:ts": "NODE_ENV=production tsup",
    "build:copy": "node src/copy.js",
    "build:watch": "NODE_ENV=development tsup --watch",
    "generate": "prisma generate --schema=src/prisma/schema.prisma",
    "prisma:migrate": "prisma migrate dev --schema=src/prisma/schema.prisma",
    "prisma:push": "prisma db push --schema=src/prisma/schema.prisma",
    "prisma:reset": "prisma reset --schema=src/prisma/schema.prisma",
    "prisma:studio": "prisma studio --schema=src/prisma/schema.prisma",
    "size": "size-limit"
  },
  "engines": {
    "node": "=18"
  },
  "size-limit": [
    {
      "path": "dist/index.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@mermaid-js/mermaid-cli": "9.1.5",
    "@size-limit/preset-small-lib": "8.0.1",
    "@tsconfig/node18": "1.0.1",
    "@tsconfig/recommended": "1.0.1",
    "npm-run-all": "4.1.5",
    "prisma": "4.3.1",
    "prisma-erd-generator": "1.1.2",
    "size-limit": "8.0.1",
    "tsup": "6.2.2",
    "typescript": "4.8.2"
  },
  "dependencies": {
    "@prisma/client": "4.3.1"
  },
  "volta": {
    "extends": "../../package.json"
  }
}
