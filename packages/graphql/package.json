{
  "name": "@klicker-uzh/graphql",
  "version": "3.0.0-alpha.42",
  "license": "AGPL-3.0",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "analyze": "size-limit --why",
    "prebuild": "npm run generate",
    "build": "NODE_ENV=production tsup --dts",
    "dev": "npm-run-all --parallel dev:graphql dev:ts",
    "dev:ts": "NODE_ENV=development doppler run --config dev -- tsup --watch --dts",
    "dev:graphql": "doppler run --config dev -- graphql-codegen --config codegen.ts --watch '**/**.graphql' --watch '**/**.ts'",
    "format": "prettier --write .",
    "generate": "graphql-codegen --config codegen.ts",
    "push": "ts-node --esm src/lib/push.ts",
    "size": "size-limit"
  },
  "engines": {
    "node": ">=18"
  },
  "size-limit": [
    {
      "path": "dist/index.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/ops.js",
      "limit": "10 KB"
    }
  ],
  "peerDependencies": {
    "@klicker-uzh/grading": "^3.0.0-alpha.0",
    "@klicker-uzh/prisma": "^3.0.0-alpha.0",
    "graphql-yoga": "^3.6.0"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "3.0.0",
    "@graphql-codegen/fragment-matcher": "4.0.0",
    "@graphql-codegen/introspection": "3.0.0",
    "@graphql-codegen/schema-ast": "3.0.0",
    "@graphql-codegen/typed-document-node": "3.0.0",
    "@graphql-codegen/typescript": "3.0.0",
    "@graphql-codegen/typescript-operations": "3.0.0",
    "@graphql-codegen/typescript-resolvers": "3.0.0",
    "@size-limit/preset-small-lib": "8.2.4",
    "@tsconfig/node18": "^1.0.1",
    "@tsconfig/recommended": "^1.0.2",
    "@types/bcrypt": "^5.0.0",
    "@types/bcryptjs": "^2.4.2",
    "@types/express": "^4.17.13",
    "@types/jest": "^29.0.0",
    "@types/mathjs": "9.4.2",
    "@types/node-schedule": "^2.1.0",
    "@types/validator": "^13.7.5",
    "@types/web-push": "^3.3.2",
    "dotenv": "16.0.3",
    "graphql-codegen-persisted-query-ids": "0.1.2",
    "husky": "8.0.3",
    "ioredis": "5.3.1",
    "jest": "29.4.2",
    "npm-run-all": "4.1.5",
    "size-limit": "8.2.4",
    "ts-node": "10.9.1",
    "ts-node-dev": "2.0.0",
    "tsup": "6.6.2",
    "typescript": "4.9.5",
    "web-push": "3.5.0"
  },
  "dependencies": {
    "@pothos/core": "3.25.0",
    "@pothos/plugin-prisma": "3.41.0",
    "@pothos/plugin-scope-auth": "3.18.0",
    "@pothos/plugin-validation": "3.9.0",
    "bcryptjs": "2.4.3",
    "dayjs": "1.11.7",
    "generate-password": "1.7.0",
    "graphql": "16.6.0",
    "graphql-scalars": "1.20.1",
    "mathjs": "11.5.1",
    "node-schedule": "2.1.1",
    "validator": "13.9.0",
    "zod": "3.20.6"
  },
  "volta": {
    "extends": "../../package.json"
  }
}
