{
  "name": "@klicker-uzh/graphql",
  "version": "3.0.0-alpha.21",
  "license": "AGPL-3.0",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "analyze": "size-limit --why",
    "prebuild": "npm run generate",
    "build": "NODE_ENV=production tsup --dts",
    "dev": "doppler run --config dev -- npm-run-all --parallel dev:graphql dev:ts",
    "dev:ts": "NODE_ENV=development tsup --watch --dts",
    "dev:graphql": "graphql-codegen --config codegen.ts --watch '**/**.graphql' --watch '**/**.ts'",
    "format": "prettier --write .",
    "generate": "graphql-codegen --config codegen.ts",
    "push": "ts-node --esm src/lib/push.ts",
    "size": "size-limit"
  },
  "engines": {
    "node": ">=18"
  },
  "size-limit": [
    {
      "path": "dist/index.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/ops.js",
      "limit": "10 KB"
    }
  ],
  "peerDependencies": {
    "@klicker-uzh/grading": "^3.0.0-alpha.0",
    "@klicker-uzh/prisma": "^3.0.0-alpha.0",
    "graphql-yoga": "^3.0.0"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "2.16.2",
    "@graphql-codegen/fragment-matcher": "3.3.3",
    "@graphql-codegen/introspection": "2.2.3",
    "@graphql-codegen/schema-ast": "2.6.1",
    "@graphql-codegen/typed-document-node": "2.3.11",
    "@graphql-codegen/typescript": "2.8.6",
    "@graphql-codegen/typescript-operations": "2.5.11",
    "@graphql-codegen/typescript-react-apollo": "3.3.7",
    "@graphql-codegen/typescript-resolvers": "2.7.11",
    "@size-limit/preset-small-lib": "8.1.2",
    "@tsconfig/node18": "1.0.1",
    "@tsconfig/recommended": "1.0.1",
    "@types/bcrypt": "^5.0.0",
    "@types/bcryptjs": "^2.4.2",
    "@types/express": "^4.17.13",
    "@types/jest": "^29.0.0",
    "@types/mathjs": "9.4.2",
    "@types/node-schedule": "^2.1.0",
    "@types/validator": "^13.7.5",
    "@types/web-push": "^3.3.2",
    "dotenv": "16.0.3",
    "graphql-codegen-persisted-query-ids": "0.1.2",
    "husky": "8.0.1",
    "ioredis": "5.2.4",
    "jest": "29.2.1",
    "npm-run-all": "4.1.5",
    "size-limit": "8.1.2",
    "ts-node": "10.9.1",
    "ts-node-dev": "2.0.0",
    "tsup": "6.6.0",
    "typescript": "4.9.5",
    "web-push": "3.5.0"
  },
  "dependencies": {
    "@pothos/core": "3.24.0",
    "@pothos/plugin-prisma": "3.39.0",
    "@pothos/plugin-scope-auth": "3.16.0",
    "@pothos/plugin-validation": "3.8.7",
    "bcryptjs": "2.4.3",
    "dayjs": "1.11.6",
    "generate-password": "1.7.0",
    "graphql": "16.6.0",
    "graphql-scalars": "1.20.0",
    "mathjs": "11.4.0",
    "node-schedule": "2.1.0",
    "validator": "13.7.0",
    "zod": "3.20.2"
  },
  "volta": {
    "extends": "../../package.json"
  }
}
