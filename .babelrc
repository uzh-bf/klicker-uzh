{
  "env": {
    "development": {
      "presets": [
        "next/babel",
        {
          "styled-jsx": {
            "plugins": ["styled-jsx-plugin-sass"]
          }
        }
      ],
      "plugins": ["inline-dotenv", "add-react-displayname", "react-intl"]
    },
    "production": {
      "presets": [
        "next/babel",
        {
          "styled-jsx": {
            "plugins": ["styled-jsx-plugin-sass"]
          }
        }
      ],
      "plugins": [
        ["lodash", { "id": ["lodash", "recompose"] }],
        "transform-inline-environment-variables",
        "add-react-displayname",
        [
          "react-intl",
          {
            "messagesDir": "src/lang/.messages"
          }
        ]
      ]
    },
    "test": {
      "presets": [
        [
          "env",
          {
            "modules": "commonjs"
          }
        ],
        "next/babel",
        {
          "styled-jsx": {
            "plugins": ["styled-jsx-plugin-sass"]
          }
        }
      ]
    }
  }
}
