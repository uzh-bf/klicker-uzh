{
  "env":{
     "development":{
        "presets":[
           [
              "next/babel",
              {
                 "preset-env":{
                    "targets":{
                       "browsers":[
                          "> 0.25%",
                          "not dead"
                       ]
                    }
                 },
                 "styled-jsx":{
                    "plugins":[
                       "styled-jsx-plugin-sass"
                    ]
                 }
              },

           ]
        ],
        "plugins":[
           "inline-dotenv",
           "add-react-displayname"
        ]
     },
     "production":{
        "presets":[
           [
              "next/babel",
              {
                 "preset-env":{
                    "targets":{
                       "browsers":[
                          "> 0.25%",
                          "not dead"
                       ]
                    }
                 },
                 "styled-jsx":{
                    "plugins":[
                       "styled-jsx-plugin-sass"
                    ]
                 }
              },
           ]
        ],
        "plugins":[
           [
              "lodash",
              {
                 "id":[
                    "lodash",
                    "recompose",
                    "semantic-ui-react",
                    "ramda"
                 ]
              }
           ],
           "transform-inline-environment-variables",
           "add-react-displayname",
           [
              "react-intl",
              {
                 "messagesDir":"src/lang/.messages"
              }
           ]
        ]
     },
     "test":{
        "presets":[
           [

              "@babel/preset-env",
              {
                 "modules":"commonjs"
              },
           ],
           [
              "next/babel",
              {
                 "preset-env":{
                    "modules":"commonjs"
                 },
                 "styled-jsx":{
                    "plugins":[
                       "styled-jsx-plugin-sass"
                    ]
                 }
              }
           ]
        ],
        "plugins":[
           "import-graphql"
        ]
     }
  }
}
