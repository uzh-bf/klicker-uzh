{
  "autoInstall": true,
  "base": "src\\lib\\semantic",
  "paths": {
    "source": {
      "config": "src/theme.config",
      "definitions": "src/definitions/",
      "site": "src/site/",
      "themes": "src/themes/"
    },
    "output": {
      "packaged": "dist\\",
      "uncompressed": "dist\\components\\",
      "compressed": "dist\\components\\",
      "themes": "dist\\themes\\"
    },
    "clean": "dist/"
  },
  "permission": false,
  "rtl": false,
  "version": "2.3.1",
  "components": [
    "reset",
    "site",
    "button",
    "divider",
    "flag",
    "header",
    "icon",
    "image",
    "input",
    "label",
    "list",
    "loader",
    "form",
    "grid",
    "menu",
    "message",
    "table",
    "item",
    "checkbox",
    "dimmer",
    "dropdown",
    "embed",
    "modal",
    "popup",
    "sidebar",
    "tab",
    "transition"
  ]
}
