{
  "name": "desktop",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "build": "tsc",
    "clean": "rimraf dist",
    "prestart": "npm run clean && npm run build",
    "dev": "npm-run-all --parallel dev:ts dev:func",
    "dev:ts": "tsc -w",
    "dev:func": "func start -p 7073",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "start": "func start -p 7073",
    "check": "tsc --noEmit"
  },
  "dependencies": {
    "@azure/functions": "^4.0.0-alpha.7"
  },
  "devDependencies": {
    "@types/node": "^18.x",
    "typescript": "^4.0.0",
    "rimraf": "^5.0.0"
  },
  "main": "dist/src/functions/*.js"
}
