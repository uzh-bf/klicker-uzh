    "jsonwebtoken": "9.0.1",
    "md5": "2.3.0",
    "ramda": "0.29.0"
  },
  "devDependencies": {
    "@tsconfig/node18": "18.2.0",
    "@tsconfig/recommended": "1.0.2",
    "@types/jsonwebtoken": "^9.0.1",
    "@types/md5": "2.3.2",
    "@types/node": "^18.11.9",
    "@types/ramda": "^0.29.0",
    "@uzh-bf/design-system": "2.0.12",
    "azure-functions-core-tools": "4.0.5198",
    "cross-env": "7.0.3",
    "dotenv": "16.0.3",
    "husky": "8.0.3",
    "npm-run-all": "4.1.5",
    "rimraf": "^5.0.0",
    "ts-node": "10.9.1",
    "tsup": "7.2.0",
