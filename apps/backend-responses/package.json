    "@types/node": "^18.11.9",
    "@uzh-bf/design-system": "2.0.12",
    "azure-functions-core-tools": "4.0.5274",
    "cross-env": "7.0.3",
    "dotenv": "16.0.3",
    "npm-run-all": "4.1.5",
    "prettier": "2.8.8",
    "prettier-plugin-organize-imports": "3.2.2",
    "tsup": "7.2.0",
    "typescript": "5.1.6"
