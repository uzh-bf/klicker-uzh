{
  "name": "@klicker-uzh/backend-docker",
  "version": "2.0.0-alpha.35",
  "license": "AGPL-3.0",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "npm-run-all --parallel dev:build dev:run",
    "dev:build": "NODE_ENV=development tsup --watch",
    "dev:run": "nodemon -w dist/ -w '../../packages/graphql/dist' --exec 'NODE_ENV=development node -r dotenv/config dist/index.js'",
    "build": "NODE_ENV=production tsup",
    "format": "prettier --write ."
  },
  "engines": {
    "node": "=18"
  },
  "size-limit": [
    {
      "path": "dist/index.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@graphql-hive/cli": "0.19.0",
    "@tsconfig/node18": "1.0.1",
    "@tsconfig/recommended": "1.0.1",
    "@types/cookie-parser": "^1.4.3",
    "@types/express": "^4.17.13",
    "@types/ioredis-mock": "^5.6.0",
    "@types/jest": "^29.0.0",
    "@types/jsonwebtoken": "^8.5.9",
    "@types/node": "^18.0.0",
    "@types/passport": "^1.0.10",
    "@types/passport-jwt": "^3.0.6",
    "@uzh-bf/design-system": "0.0.43",
    "axios": "0.27.2",
    "dotenv": "16.0.2",
    "dts-cli": "1.6.0",
    "husky": "8.0.1",
    "ioredis-mock": "8.2.2",
    "jest": "29.0.3",
    "nodemon": "2.0.20",
    "npm-run-all": "4.1.5",
    "prettier": "2.7.1",
    "prettier-plugin-organize-imports": "3.1.1",
    "prisma": "4.3.1",
    "tslib": "2.4.0",
    "tsup": "6.2.3",
    "typescript": "4.8.3"
  },
  "dependencies": {
    "@envelop/parser-cache": "4.7.0",
    "@envelop/response-cache": "3.2.0",
    "@envelop/response-cache-redis": "1.2.0",
    "@envelop/validation-cache": "4.7.0",
    "@graphql-authz/core": "1.3.0",
    "@graphql-authz/envelop-plugin": "1.0.4",
    "@graphql-hive/client": "0.18.3",
    "@graphql-yoga/node": "2.13.13",
    "@klicker-uzh/graphql": "^2.0.0-alpha.8",
    "@klicker-uzh/prisma": "^2.0.0-alpha.9",
    "cookie-parser": "1.4.6",
    "express": "4.18.1",
    "graphql": "16.6.0",
    "ioredis": "5.2.3",
    "jsonwebtoken": "8.5.1",
    "passport": "0.6.0",
    "passport-jwt": "4.0.0"
  },
  "volta": {
    "extends": "../../package.json"
  },
  "overrides": {
    "@envelop/core": "^2.5.0"
  }
}
