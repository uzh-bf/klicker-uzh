  "scripts": {
    "build": "cross-env NODE_ENV=production run-s build:ts",
    "build:instrument": "nyc instrument --compact=false src instrumented",
    "build:test": "run-s build:instrument build:ts",
    "build:ts": "tsup",
    "check": "tsc --noEmit",
    "dev": "npm-run-all --parallel dev:build dev:run",
    "dev:build": "tsup --watch",
    "dev:doppler": "doppler run --config dev -- pnpm run dev",
    "dev:lti": "pnpm run dev",
    "dev:offline": "pnpm run dev",
    "dev:run": "nodemon -w dist/ -w '../../packages/graphql/dist' --exec 'node ./dist/index.js'",
    "dev:test": "cross-env NODE_ENV=test pnpm run dev",
    "script": "doppler run --config dev -- tsx",
    "script:prod": "doppler run --config prd -- tsx",
    "start": "node -r dotenv/config dist/index.js",
    "start:test": "nyc --silent node -r dotenv/config dist/index.js"
  },
  "engines": {
    "node": "=20"
  },
  "volta": {
    "extends": "../../package.json"
  },
  "type": "module"
