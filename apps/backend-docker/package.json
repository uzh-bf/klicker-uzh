{
  "name": "@klicker-uzh/backend-docker",
  "version": "3.0.0-beta.3",
  "license": "AGPL-3.0",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "npm-run-all --parallel dev:build dev:run",
    "dev:offline": "npm-run-all --parallel dev:build dev:run",
    "dev:build": "tsup --watch",
    "dev:run": "nodemon -w dist/ -w '../../packages/graphql/dist' --exec 'node -r dotenv/config ./dist/index.js'",
    "build": "cross-env NODE_ENV=production tsup",
    "build:test": "npm run build",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "start": "node -r dotenv/config dist/index.js",
    "start:test": "nyc --silent npm start",
    "check": "tsc --noEmit"
  },
  "engines": {
    "node": "=18"
  },
  "size-limit": [
    {
      "path": "dist/index.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@tsconfig/node18": "^2.0.0",
    "@tsconfig/recommended": "^1.0.2",
    "@types/cookie-parser": "^1.4.3",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/ioredis-mock": "^8.2.1",
    "@types/jest": "^29.0.0",
    "@types/jsonwebtoken": "^9.0.1",
    "@types/node": "^18.0.0",
    "@types/passport": "^1.0.10",
    "@types/passport-jwt": "^3.0.6",
    "@uzh-bf/design-system": "2.0.2",
    "axios": "1.4.0",
    "cross-env": "7.0.3",
    "dotenv": "16.0.3",
    "husky": "8.0.3",
    "ioredis-mock": "8.7.0",
    "jest": "29.5.0",
    "nodemon": "2.0.22",
    "npm-run-all": "4.1.5",
    "prisma": "4.14.1",
    "tsup": "6.7.0",
    "typescript": "5.0.4"
  },
  "dependencies": {
    "@envelop/response-cache-redis": "2.0.8",
    "@envelop/sentry": "5.1.1",
    "@escape.tech/graphql-armor": "2.2.0",
    "@graphql-yoga/plugin-csrf-prevention": "1.5.1",
    "@graphql-yoga/plugin-persisted-operations": "1.9.1",
    "@graphql-yoga/plugin-response-cache": "1.9.1",
    "@graphql-yoga/redis-event-target": "1.0.0",
    "@klicker-uzh/graphql": "^3.0.0-alpha.0",
    "@klicker-uzh/prisma": "^3.0.0-alpha.0",
    "@sentry/node": "7.52.1",
    "@sentry/tracing": "7.52.1",
    "cookie-parser": "1.4.6",
    "cors": "2.8.5",
    "express": "4.18.2",
    "graphql": "16.6.0",
    "graphql-ws": "5.13.1",
    "graphql-yoga": "3.9.1",
    "ioredis": "5.3.2",
    "jsonwebtoken": "9.0.0",
    "passport": "0.6.0",
    "passport-jwt": "4.0.1",
    "ws": "8.13.0"
  },
  "volta": {
    "extends": "../../package.json"
  }
}
