    "morgan": "1.10.0",
    "node-schedule": "2.0.0",
    "nodemailer": "6.6.3",
    "object-hash": "3.0.0",
    "pretty-error": "2.1.2",
    "ramda": "0.27.1",
    "rate-limit-redis": "2.1.0",
    "raven": "2.6.4",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "request": "2.88.2",
    "request-promise": "4.2.6",
    "saslprep": "1.0.3",
    "unescape": "1.0.1",
