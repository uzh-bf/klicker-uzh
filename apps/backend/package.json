    "elastic-apm-node": "3.20.0",
    "express": "4.17.1",
    "express-jwt": "6.1.0",
    "express-rate-limit": "5.3.0",
    "generate-password": "1.6.1",
    "graphql": "15.5.3",
    "graphql-middleware": "6.1.6",
    "graphql-redis-subscriptions": "2.4.0",
    "graphql-scalars": "1.10.1",
    "graphql-shield": "7.5.0",
    "graphql-subscriptions": "1.2.1",
    "graphql-ws": "5.4.1",
    "handlebars": "4.7.7",
    "helmet": "4.6.0",
    "ioredis": "4.27.9",
    "jsonwebtoken": "8.5.1",
    "lodash": "4.17.21",
    "md5": "2.3.0",
