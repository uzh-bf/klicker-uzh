{
  "name": "@klicker-uzh/office-addin",
  "version": "3.0.0-alpha.80",
  "license": "AGPL-3.0",
  "config": {
    "app_to_debug": "powerpoint",
    "app_type_to_debug": "desktop",
    "dev_server_port": 3000
  },
  "scripts": {
    "build": "webpack --mode production",
    "build:dev": "webpack --mode development",
    "dev-server": "webpack serve --mode development",
    "lint": "office-addin-lint check",
    "lint:fix": "office-addin-lint fix",
    "prettier": "office-addin-lint prettier",
    "start:content": "office-addin-debugging start manifest-content.xml",
    "start:taskpane": "office-addin-debugging start manifest-taskpane.xml",
    "start:desktop": "office-addin-debugging start manifest-content.xml desktop",
    "start:web": "office-addin-debugging start manifest-content.xml web",
    "stop:content": "office-addin-debugging stop manifest-content.xml",
    "stop:taskpane": "office-addin-debugging stop manifest-taskpane.xml",
    "stop": "npm-run-all --parallel stop:content stop:taskpane",
    "validate:content": "office-addin-manifest validate manifest-content.xml",
    "validate:taskpane": "office-addin-manifest validate manifest-taskpane.xml",
    "watch": "webpack --mode development --watch",
    "clear": ".\\scripts\\clear.bat",
    "dev:tailwind": "tailwindcss -i ./src/content/content.css -o ./src/content/output.css --watch",
    "dev-addin": "npm-run-all --parallel dev:tailwind start:content start:taskpane",
    "predev": "npm run clear",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "check": "tsc --noEmit"
  },
  "dependencies": {
    "@fluentui/react": "8.105.7",
    "@uzh-bf/design-system": "1.6.0",
    "core-js": "3.27.2",
    "es6-promise": "4.2.8",
    "formik": "2.2.9",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "regenerator-runtime": "0.13.11"
  },
  "devDependencies": {
    "@babel/core": "7.20.12",
    "@babel/preset-typescript": "7.18.6",
    "@tailwindcss/aspect-ratio": "0.4.2",
    "@tailwindcss/forms": "0.5.3",
    "@tailwindcss/line-clamp": "0.4.2",
    "@tailwindcss/typography": "0.5.9",
    "@types/node": "^18.11.18",
    "@types/office-js": "^1.0.256",
    "@types/office-runtime": "^1.0.23",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "@types/react-hot-loader": "^4.1.1",
    "@types/webpack": "^5.0.0",
    "@types/webpack-dev-server": "^4.1.0",
    "acorn": "8.8.2",
    "autoprefixer": "10.4.13",
    "babel-loader": "9.1.2",
    "copy-webpack-plugin": "11.0.0",
    "eslint": "8.34.0",
    "eslint-plugin-office-addins": "2.1.4",
    "eslint-plugin-react": "7.32.2",
    "file-loader": "6.2.0",
    "html-loader": "4.2.0",
    "html-webpack-plugin": "5.5.0",
    "npm-run-all": "4.1.5",
    "office-addin-cli": "1.5.4",
    "office-addin-debugging": "4.6.7",
    "office-addin-dev-certs": "1.11.1",
    "office-addin-lint": "2.2.4",
    "office-addin-manifest": "1.12.0",
    "office-addin-prettier-config": "1.2.0",
    "os-browserify": "0.3.0",
    "postcss": "8.4.21",
    "prettier": "2.8.4",
    "prettier-plugin-organize-imports": "3.2.2",
    "process": "0.11.10",
    "source-map-loader": "4.0.1",
    "tailwindcss": "3.2.6",
    "tailwindcss-radix": "2.7.0",
    "ts-loader": "9.4.2",
    "typescript": "4.9.5",
    "webpack": "5.75.0",
    "webpack-cli": "5.0.1",
    "webpack-dev-server": "4.11.1"
  },
  "prettier": "office-addin-prettier-config",
  "browserslist": [
    "ie 11"
  ],
  "volta": {
    "extends": "../../package.json"
  },
  "engines": {
    "node": ">=16"
  }
}
