{
  "name": "office-addin-taskpane-react",
  "version": "0.0.1",
  "repository": {
    "type": "git",
    "url": "https://github.com/OfficeDev/Office-Addin-TaskPane-React.git"
  },
  "license": "MIT",
  "config": {
    "app_to_debug": "powerpoint",
    "app_type_to_debug": "desktop",
    "dev_server_port": 3000
  },
  "scripts": {
    "build": "webpack --mode production",
    "build:dev": "webpack --mode development",
    "dev-server": "webpack serve --mode development",
    "lint": "office-addin-lint check",
    "lint:fix": "office-addin-lint fix",
    "prettier": "office-addin-lint prettier",
    "start:content": "office-addin-debugging start manifest-content.xml",
    "start:taskpane": "office-addin-debugging start manifest-taskpane.xml",
    "start:desktop": "office-addin-debugging start manifest-content.xml desktop",
    "start:web": "office-addin-debugging start manifest-content.xml web",
    "stop:content": "office-addin-debugging stop manifest-content.xml",
    "stop:taskpane": "office-addin-debugging stop manifest-taskpane.xml",
    "stop": "npm-run-all --parallel stop:content stop:taskpane",
    "validate:content": "office-addin-manifest validate manifest-content.xml",
    "validate:taskpane": "office-addin-manifest validate manifest-taskpane.xml",
    "watch": "webpack --mode development --watch",
    "clear": ".\\scripts\\clear.bat",
    "dev:tailwind": "tailwindcss -i ./src/content/content.css -o ./src/content/output.css --watch",
    "dev": "npm-run-all --parallel dev:tailwind start:content start:taskpane",
    "predev": "npm run clear"
  },
  "dependencies": {
    "@fluentui/react": "^8.52.3",
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@tailwindcss/forms": "^0.5.3",
    "@tailwindcss/line-clamp": "^0.4.2",
    "@tailwindcss/typography": "^0.5.8",
    "@uzh-bf/design-system": "0.1.20",
    "core-js": "^3.9.1",
    "es6-promise": "^4.2.8",
    "eslint": "^7.32.0",
    "formik": "^2.2.9",
    "prettier-plugin-organize-imports": "^3.2.1",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "regenerator-runtime": "^0.13.7",
    "tailwindcss-radix": "^2.6.1"
  },
  "devDependencies": {
    "@babel/core": "^7.13.10",
    "@babel/preset-typescript": "^7.13.0",
    "@types/node": "^18.11.18",
    "@types/office-js": "^1.0.256",
    "@types/office-runtime": "^1.0.23",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "@types/react-hot-loader": "^4.1.1",
    "@types/webpack": "^4.4.34",
    "@types/webpack-dev-server": "^4.1.0",
    "acorn": "^8.5.0",
    "autoprefixer": "^10.4.13",
    "babel-loader": "^8.2.2",
    "copy-webpack-plugin": "^9.0.1",
    "eslint-plugin-office-addins": "^2.1.3",
    "eslint-plugin-react": "^7.28.0",
    "file-loader": "^6.2.0",
    "html-loader": "^4.1.0",
    "html-webpack-plugin": "^5.5.0",
    "less": "^3.9.0",
    "less-loader": "^10.0.1",
    "npm-run-all": "^4.1.5",
    "office-addin-cli": "^1.5.3",
    "office-addin-debugging": "^4.6.3",
    "office-addin-dev-certs": "^1.9.3",
    "office-addin-lint": "^2.2.3",
    "office-addin-manifest": "^1.10.4",
    "office-addin-prettier-config": "^1.2.0",
    "os-browserify": "^0.3.0",
    "postcss": "^8.4.19",
    "process": "^0.11.10",
    "source-map-loader": "^3.0.0",
    "tailwindcss": "^3.2.4",
    "ts-loader": "^9.4.1",
    "typescript": "^4.3.5",
    "webpack": "^5.73.0",
    "webpack-cli": "^4.8.0",
    "webpack-dev-server": "4.7.4"
  },
  "prettier": "office-addin-prettier-config",
  "browserslist": [
    "ie 11"
  ]
}
