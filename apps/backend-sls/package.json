{
  "name": "@klicker-uzh/backend-sls",
  "version": "2.0.0-alpha.0",
  "license": "AGPL-3.0",
  "main": "dist/index.js",
  "module": "dist/backend-sls.esm.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "dts build --entry src/index.ts",
    "lint": "dts lint",
    "size": "size-limit",
    "dev": "npm-run-all --parallel dev:dts dev:func",
    "dev:dts": "dts watch --entry src/index.ts",
    "dev:func": "func start",
    "generate": "npm-run-all generate:prisma generate:nexus",
    "generate:prisma": "prisma generate",
    "generate:nexus": "ts-node --transpile-only -P tsconfig.nexus.json src/app.ts",
    "test": "dts test"
  },
  "dependencies": {
    "@graphql-authz/core": "1.2.1",
    "@graphql-authz/envelop-plugin": "1.0.3",
    "@graphql-yoga/node": "2.13.4",
    "@prisma/client": "4.1.1",
    "@vendia/serverless-express": "4.10.1",
    "cookie-parser": "1.4.6",
    "express": "4.18.1",
    "graphql": "16.5.0",
    "nexus": "1.3.0",
    "passport": "0.6.0",
    "passport-jwt": "4.0.0"
  },
  "devDependencies": {
    "@azure/functions": "3.2.0",
    "@tsconfig/recommended": "1.0.1",
    "@types/cookie-parser": "1.4.3",
    "@types/express": "4.17.13",
    "@types/node": "18.6.2",
    "@types/passport": "1.0.9",
    "@types/passport-jwt": "3.0.6",
    "dts-cli": "1.5.2",
    "husky": "8.0.1",
    "jest": "28.1.3",
    "npm-run-all": "4.1.5",
    "prisma": "4.1.1",
    "size-limit": "8.0.0",
    "supertest": "6.2.4",
    "ts-jest": "28.0.7",
    "ts-node": "10.9.1",
    "tslib": "2.4.0",
    "typescript": "4.7.4"
  },
  "volta": {
    "extends": "../../package.json"
  },
  "jest": {
    "testEnvironment": "node"
  },
  "engines": {
    "node": ">=16"
  }
}
