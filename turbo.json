{
  "$schema": "https://turborepo.org/schema.json",
  "pipeline": {
    "lint:staged": {
      "dependsOn": ["lint:staged"],
      "outputs": []
    },
    "test:changed": {
      "dependsOn": ["test:changed"],
      "outputs": []
    },
    "build": {
      "dependsOn": ["^build", "generate"],
      "outputs": ["dist/**", ".next/**"]
    },
    "dev": {
      "cache": false
    }
  }
}
