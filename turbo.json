{
  "$schema": "https://turborepo.org/schema.json",
  "pipeline": {
    "format": {
      "cache": false
    },
    "build": {
      "dependsOn": ["generate", "^build"],
      "outputs": ["dist/**", ".next/**"]
    },
    "dev": {
      "cache": false,
      "dependsOn": ["generate", "^build"],
      "outputs": ["dist/**", ".next/**"]
    },
    "generate": {
      "dependsOn": ["^build"],
      "outputs": ["src/client/**", "dist/**"]
    },
    "publish": {
      "cache": false,
      "dependsOn": ["build"]
    }
  }
}
