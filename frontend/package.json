  "scripts": {
    "analyze": "cross-env BUNDLE_ANALYZE=both NODE_ENV=production next build src",
    "build": "npm run extract:i18n && npm run compile:i18n && next build && tsc -p tsconfig.server.json",
    "build:semantic": "cd src/lib/semantic && gulp build-css build-assets",
    "compile-queries": "persistgraphql src/graphql/ --add_typename",
    "cypress:open": "cypress open",
    "dev": "cross-env NODE_ICU_DATA=node_modules/full-icu NODE_ENV=development nodemon --ext 'js,ts,tsx,graphql,json' -w .env -w package.json -w src/server.ts -w src/klicker.conf.js -w 'src/graphql/**/*.graphql' --exec \"rimraf .next && ts-node --project tsconfig.server.json -r dotenv/config src/server.ts\"",
    "dev:inspect": "cross-env NODE_ENV=development nodemon -w .env -w package.json -w src/server.ts -w src/klicker.conf.js --inspect --exec \"ts-node --project tsconfig.server.json -r dotenv/config src/server.ts\"",
    "dev:ndb": "cross-env NODE_ENV=development ndb ts-node --project tsconfig.server.json -r dotenv/config src/server.ts",
    "dev:no-custom-server": "next dev",
    "format": "prettier --write \"src/**/*.js\" \"src/**/*.ts\" \"src/**/*.tsx\" \"src/**/*.graphql\" \"src/**/*.json\" src/server.ts",
    "format:ci": "prettier --list-different \"src/**/*.js\" \"src/**/*.ts\" \"src/**/*.tsx\" \"src/**/*.graphql\" \"src/**/*.json\" src/server.ts",
    "lint": "next lint",
    "lint:staged": "lint-staged",
    "start": "cross-env NODE_ENV=production NODE_ICU_DATA=node_modules/full-icu node -r dotenv/config dist/server",
    "start-no-custom-server": "next start",
    "start:inspect": "cross-env NODE_ENV=production node --inspect -r dotenv/config dist/server",
    "storybook": "start-storybook -p 6006",
    "test": "jest",
    "test:changed": "jest --onlyChanged",
    "test:ci": "jest --coverage",
    "test:watch": "jest --watch",
    "type-check": "tsc --noEmit -p tsconfig.json",
    "type-check:watch": "npm run type-check -- --watch",
    "build-storybook": "build-storybook",
    "extract:i18n": "formatjs extract 'src/{pages,components}/**/*.{js,ts,tsx}' --format simple --id-interpolation-pattern '[sha512:contenthash:base64:6]' --out-file lang/en.json",
    "compile:i18n": "formatjs compile-folder --ast --format simple lang compiled-lang"
  },
  "jest": {
    "testEnvironment": "node",
    "roots": [
      "src/"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/.next"
    ]
  },
  "lint-staged": {
    "*.js": [
      "prettier --write"
    ],
    "*.ts": [
      "prettier --write"
    ],
    "*.tsx": [
      "prettier --write"
    ],
    "*.json": [
      "prettier --write"
    ],
    "*.graphql": [
      "prettier --write"
    ]
  },
  "volta": {
    "node": "16.13.0"
  }
