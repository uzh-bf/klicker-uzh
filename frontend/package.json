  "scripts": {
    "analyze": "cross-env BUNDLE_ANALYZE=both NODE_ENV=production next build src",
    "build": "next build",
    "build:lang": "next build && node ./scripts/extract '{pages,components}/*.{js,ts,tsx}'",
    "build:semantic": "cd src/lib/semantic && gulp build-css build-assets",
    "compile-queries": "persistgraphql src/graphql/ --add_typename",
    "cypress:open": "cypress open",
    "dev": "cross-env NODE_ENV=development nodemon -w .env -w package.json -w server.js -w src/klicker.conf.js server.js",
    "dev:inspect": "cross-env NODE_ENV=development nodemon -w .env -w package.json -w server.js -w src/klicker.conf.js --inspect server.js",
    "dev:ndb": "cross-env NODE_ENV=development ndb node server.js",
    "format": "prettier --write \"src/**/*.js\" \"src/**/*.ts\" \"src/**/*.tsx\" \"src/**/*.graphql\" \"src/**/*.json\" server.js",
    "format:ci": "prettier --list-different \"src/**/*.js\" \"src/**/*.ts\" \"src/**/*.tsx\" \"src/**/*.graphql\" \"src/**/*.json\" server.js",
    "gql-gen": "gql-gen",
    "lint": "next lint",
    "start": "cross-env NODE_ENV=production node server.js",
    "start:inspect": "cross-env NODE_ENV=production node --inspect server.js",
    "storybook": "start-storybook -p 6006",
    "test": "jest",
    "test:ci": "jest --coverage",
    "test:watch": "jest --watch",
    "type-check": "tsc --noEmit -p tsconfig.json",
    "type-check:watch": "npm run type-check -- --watch",
    "build-storybook": "build-storybook"
  },
  "jest": {
    "testEnvironment": "node",
    "roots": [
      "src/"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/.next"
    ]
  },
  "lint-staged": {
    "*.js": [
      "prettier --write"
    ],
    "*.ts": [
      "prettier --write"
    ],
    "*.tsx": [
      "prettier --write"
    ],
    "*.json": [
      "prettier --write"
    ],
    "*.graphql": [
      "prettier --write"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "jest --onlyChanged"
    }
  }
}
